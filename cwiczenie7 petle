#include <stdio.h>
int main(void)
{
int result[6], i, y;
i = 0;
while (i < 6)

        {
                printf("podaj liczbe: ");
                scanf("%d", &result[i]);
                ++i;
        }
                for (i = 0; i < 6; ++i)
                {
                if (result[i] >= 0)
                        ++y;
                }
        printf("Ilosc liczb rzeczywistych to : %d ", y);
return 0;
}
